.tan_mask {
	width: 100%;
	position: fixed;
	top: 0;
	left: 0;
	background: rgba(0, 0, 0, 0.5);
	height: 100%;
	z-index: 999;
}

.tan_wrap {
	width: 100%;
	background: #fff;
	position: fixed;
	bottom: 0;
	left: 0;
	z-index: 99999;
	height: 560rpx;
	padding: 0 30rpx;
	box-sizing: border-box;
}

.tan_title_wrap {
	font-size: 32rpx;
	color: #565656;
	line-height: 80rpx;
	width: 100%;
	position: absolute;
	top: 0;
	left: 0;
	padding: 0 30rpx;
	box-sizing: border-box;
}

.jiaru_auto {
	width: 100%;
	height: 480rpx;
	overflow: auto;
}

.jiaru_wrap {
	width: 100%;
	/* height: 140rpx; */
	border-radius: 5px;
	border-bottom: 1px solid #d2d2d2;
	/* box-shadow: 1px 1px 8px #d2d2d2; */
	padding: 30rpx 20rpx;
	box-sizing: border-box;
	position: relative;
	margin-bottom: 20rpx;
}

.jiaru_user {
	width: 70%;
	height: 42rpx;
	font-size: 30rpx;
	color: #000;
	line-height: 42rpx;
}

.jiaru_user image {
	width: 42rpx;
	height: 42rpx;
	border-radius: 50%;
	display: block;
	float: left;
	margin-right: 10rpx;

}

.jiaru_hist {
	font-size: 28rpx;
	color: #8c8c8c;
	/* margin-top: 16rpx; */
}

.jiaru_but {
	width: 120rpx;
	height: 60rpx;
	border-radius: 30px;
	border: 1px solid #dcdcdc;
	background: #f4f4f4;
	font-size: 24rpx;
	color: #000;
	position: absolute;
	top: 0;
	right: 20rpx;
	bottom: 0;
	margin: auto;
	text-align: center;
	line-height: 60rpx;
}

.jiaru_butOn {
	color: #fff !important;
	background: #3e9a37 !important;
	border: 1px solid #3e9a37 !important;
}

::-webkit-scrollbar {
	width: 0;
	height: 0;
	color: transparent;
}
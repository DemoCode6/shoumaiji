}

.actives .img {
	width: 20%;
	height: 100%;
	text-align: center;
	float: left;
}

.actives .img image {
	width: 75rpx;
	margin-top: 37.5rpx;
}

.actives .opc {
	width: 80%;
	float: left;
	height: 100%;
}

.actives .opc view {
	color: #b6880e;
}


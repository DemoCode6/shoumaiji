/* pages/productdet/productdet.wxss */

.pro_price {
  width: 100%;
  overflow: hidden;
  background-color: white;
  /*z-index: 99;*/
  position: relative;
  padding: 11.25rpx 0;
}

.pro_price .nnd {
  width: 100%;
  overflow: hidden;
  margin: 18.75rpx 0;
  background-color: white;
}

.pro_price .nnb {
  width: 100%;
  height: 67.5rpx;
}

.pro_price .nnb text {
  float: left;
  margin-left: 11.25rpx;
  color: #ff4e42;
  margin-top: 11.25rpx;
  font-size: 24px;
}

.pro_price .nnd text {
  display: inline-block;
  width: 33.3%;
  float: left;
  margin-left: 2.5%;
  color: #999;
  margin-top: 9.75rpx;
  font-size: 12px;
}

.pro_price .nnd .img_cont {
  width: 60rpx;
  height: 100%;
  float: right;
  text-align: center;
  margin-right: 11.25rpx;
}

.pro_price .nnd .img_cont image {
  width: 30rpx;
  margin-top: 9.75rpx;
}

.pro_price .nnc {
  width: 95%;
  margin-left: 2.5%;
  overflow: hidden;
  margin-top: 11.25rpx;
  line-height: 41.25rpx;
}

.pro_price .nnc text {
  color: #051b28;
  font-size: 28.5rpx !important;
  /* font-weight: 600; */
}

.pro_price .nnc .what {
  display: inline-block;
  height: 30rpx;
  border-radius: 45rpx;
  background-color: #ff4e42;
  color: white;
  padding: 0 11.25rpx;
  font-size: 22.5rpx !important;
  line-height: 30rpx;
  margin-top: 7.5rpx;
  margin-right: 11.25rpx;
}

.pro_price .td {
  color: #c1c1c1;
  width: 95%;
  margin-left: 2.5%;
  margin-top: 0.15rem;
}

.pro_price .td text {
  display: inline-block;
  width: 50%;
  float: left;
  color: #c1c1c1;
}

.chooses {
  width: 95%;
  margin-left: 2.5%;
  overflow: hidden;
  border-top: 1px solid #eee;
  border-bottom: 1px solid #eee;
  padding: 9.75rpx 0;
}

.chooses .one {
  width: 100%;
  /*margin-left: 5%;*/
  border-bottom: 1px solid #eee;
  overflow: hidden;
}

.chooses .one:last-child {
  border: none;
}

.chooses .one .zd {
  width: 100%;
  overflow: hidden;
  height: 75rpx;
  line-height: 75rpx;
}

.chooses .one .zd .aa {
  color: #999;
  margin-right: 37.5rpx;
  font-size: 12px;
}

.chooses .one .zd .bb {
  font-size: 12px;
  color: #333;
}

.chooses .one .jt {
  width: 11.25rpx;
  float: right;
  margin-top: 28.5rpx;
}

.tomore {
  width: 100%;
  overflow: hidden;
  /* margin-top: 0.6rem; *//* margin-bottom: 0.25rem; */
}

.tomore .ospan {
  display: inline-block;
  float: left;
  font-size: 30rpx;
  /* margin-left: 5%; *//* border-left: 5px solid #ff5933; */
  padding-left: 9.75rpx;
  font-size: 17px;
  line-height: 20px;
  color: #333;
  font-weight: 600;
  margin: 12px 3.5%;
}

.tomore .mmb {
  float: right;
  color: #fe5c33;
  margin: 12px 18.75rpx;
}

.tomore .mmb image {
  width: 11.25rpx;
}

.proshow {
  /*width: 95%;*//*margin-left: 2.5%;*/
  overflow: hidden;
  /*border-bottom: 1px solid #eee;*/
  padding-bottom: 22.5rpx;
}

.proshow .type {
  width: 95%;
  margin: 0rem 2.5% 22.5rpx 2.5%;
  overflow: hidden;
}

.proshow .type navigator {
  float: left;
  background-color: #fee;
  color: #666;
  margin: 6px 3px;
  padding: 6px 9px;
  text-align: center;
  border-radius: 26px;
  -moz-border-radius: 26px;
  font-size: 12px;
  line-height: 14px;
}

.proshow .zz {
  width: 24px;
  height: 24px;
  border-radius: 50%;
  float: left;
  overflow: hidden;
  margin-bottom: 11.25rpx;
  margin-left: 2.5%;
}

.proshow .zz image {
  width: 100%;
  height: 100%;
}

.proshow .names {
  float: left;
  width: 50%;
  /* margin-left: 0.15rem; */
  line-height:60rpx;
  color: #636363;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  vertical-align: middle;
  padding: 5px;
  font-size: 12px;
  color: #999;
}

.proshow .plb {
  width: 95%;
  margin-left: 2.7%;
  overflow: hidden;
  /* padding: 0.1rem 0; */
}

.proshow .plb image {
  width:37.5rpx;
  /*margin-top: 0.1rem;*/
}

.proshow .plb .op {
  font-size: 13px;
  line-height: 16px;
  text-align: justify;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  -webkit-box-pack: center;
  height: 32px;
  overflow: hidden;
  word-break: break-all;
  color: #051b28;
}

.proshow .plb .oul {
  width: 100%;
  overflow: hidden;
  /* margin-top: 0.25rem; */
}

.proshow .plb .oul .oli {
  width: 24%;
  margin: 0 0.15%;
  overflow: hidden;
  float: left;
  border-radius: 5px;
  text-align: center;
}

.proshow .plb .oul .oli image {
  width: 100%;
  /*max-height: 100%;*/
}


.prodetail_mask{
	width: 100%;
	height: 100%;
	z-index: 999;
	overflow: hidden;
	background-color: rgba(0,0,0,0.6);
	position: fixed;
	top: 0;
	left: 0;
	
}

.prodetail_mask .choose_add{
	width: 100%;
	overflow: hidden;
	position: absolute;
	bottom: 0;
	background-color: white;
	border-top-left-radius: 7px;
	border-top-right-radius: 7px;
	padding-bottom: 75rpx;
	/* display: none; */
	padding-top: 37.5rpx;
}

.prodetail_mask .choose_add .prosa{
	width: 95%;
	margin-left: 2.5%;
	overflow: hidden;
	padding-bottom: 15rpx;
}

.prodetail_mask .choose_add .prosa .imga{
	width: 112.5rpx;
	height: 112.5rpx;
	border-radius: 4px;
	box-shadow: 0 0 3px 3px #f3f3f3;
	float: left;
	margin-top: 11.25rpx;
}

.prodetail_mask .choose_add .prosa .imga image{
	width: 90rpx;
	height: 90rpx;
	margin: 11.25rpx;
}

.prodetail_mask .choose_add .prosa .xx{
	width: 50%;
	float: left;
	margin-left: 37.5rpx;
	overflow: hidden;
	margin-top: 9.75rpx;
}

.prodetail_mask .choose_add .prosa .xx .pri{
	color: #ff4e42;
	font-size: 30rpx;
  width: 100%;
  overflow: hidden;
}

.prodetail_mask .choose_add .prosa .xx .pri text{
	font-size: 37.5rpx;
	color: #ff4e42;
  float: left;
}

.prodetail_mask .choose_add .cpfl,.prodetail_mask .choose_add .buynun{
	width: 95%;
	margin-left: 2.5%;
	overflow: hidden;
}

.prodetail_mask .choose_add .cpfl view{
	margin: 7.5rpx 0;
}

.prodetail_mask .choose_add .cpfl view text{
	display: inline-block;
	float: left;
	margin-right: 37.5rpx;
	background-color: #f3f3f3;
	padding: 7.5rpx 22.5rpx;
	border-radius: 45rpx;
  margin-bottom: 20rpx;
}

.prodetail_mask .choose_add .cpfl view text.foc{
	background-color: #ffe6e4;
}

.prodetail_mask .choose_add .buynun{
	margin-top: 60rpx;
	overflow: hidden;
}

.prodetail_mask .choose_add .buynun .osp{
	float: left;
}

.prodetail_mask .choose_add .buynun .nuns{
	width: 50%;
	float: right;
	text-align: right;
}

.prodetail_mask .choose_add .buynun .nuns input{
	width: 75rpx;
	text-align: center;
  float: left;
  margin-top: -8rpx;
}

.prodetail_mask .choose_add .buynun .nuns text{
	display: inline-block;
	width: 40rpx;
	height: 40rpx;
	text-align: center;
	line-height: 40rpx;
	border: 1px solid #cccccc;
  float: left;
	/* padding: 0 0 0.1rem 0; */
}

.prodetail_mask .off{
	position: absolute;
	right: 18.75rpx;
	top: 11.25rpx;
	width: 37.5rpx;
}

.prodetail_mask .choose_add .tit{
	text-align: center;
	margin: 15rpx auto;
}

.prodetail_mask .choose_add .canshu{
	width: 90%;
	margin-left: 5%;
	margin-top: 37.5rpx;
	overflow: hidden;
}

.prodetail_mask .choose_add .canshu view{
	margin-bottom: 7.5rpx;
	line-height: 37.5rpx;
	width: 100%;
	overflow: hidden;
}

.prodetail_mask .choose_add .canshu .name{
	display: inline-block;
	width: 30%;
	float: left;
	color: #b8b8b8;
}

.prodetail_mask .choose_add .canshu .ser{
	display: inline-block;
	width: 30%;
	float: left;
}

.prodetail_mask .choose_add .canshu .answer,.prodetail_mask .choose_add .canshu .do{
	display: inline-block;
	width: 70%;
	overflow: hidden;
	float: left;
}

.prodetail_mask .choose_add .oka{
	width: 80%;
	margin-left: 10%;
	text-align: center;
	line-height: 75rpx;
	color: white;
	background-color: #ff5933;
	border-radius: 37.5rpx;
	margin-top: 60rpx;
}

.prodetail_mask .choose_add .canshu .do{
	font-size: 26.25rpx;
	color: #c1c1c1;
}


.addcars .howa view{
	width: 50%;
	float: left;
	text-align: center;
	height: 75rpx;
	line-height: 75rpx;
	color: white;
}

.scrollbox {
  width: 92%;

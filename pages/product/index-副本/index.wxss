/* pages/procenter/procenter.wxss */
page{
  background-color: #fff;
}
.tomore {
  width: 100%;
  overflow: hidden;
}

.tomore .ospan {
  display: inline-block;
  float: left;
  font-size: 30rpx;
  /* margin-left: 5%; *//* border-left: 5px solid #ff5933; */
  padding-left: 9.75rpx;
  line-height: 20px;
  color: #333;
  font-weight: 600;
  margin: 12rpx 3.5%;
}

.tomore .mmb {
  float: right;
  color: #fe5c33;
  margin: 12rpx 18.75rpx;
}

.tomore .mmb image {
  width: 11.25rpx;
}

.tomore {
  text-align: center;
}

.tomore .bors {
  display: block;
  width: 75rpx;
  float: left;
  height: 22.5rpx;
  border-bottom: 1px solid;
}

.tomore text {
  float: left;
  color: #ff5933;
  font-size: 26.25rpx;
}

.actives_pro {
  width: 100%;
  overflow: hidden;
  /* margin-left: 2.5%; *//*margin-top: 0.5rem;*/
}

.actives_pro .lis {
  width: 33.33%;
  margin-bottom: 11.25rpx;
  overflow: hidden;
  border-radius: 10px;
  /*background-color: white;*/
  padding-bottom: 11.25rpx;
  padding-top: 11.25rpx;
  margin-top: 11.25rpx;
  box-shadow: 1px 1px 5px #d0cfcf;
}

.actives_pro .lis navigator {
  display: block;
  font-size: 12px;
  position: relative;
  padding: 0 6.4%;
}

.actives_pro .lis:nth-child(even) {
  float: left;
}

.actives_pro .lis:nth-child(odd) {
  float: right;
}

.actives_pro .lis .proimgs {
  width: 100%;
  height: 308rpx;
  overflow: hidden;
  margin-bottom: 11.25rpx;
  position: relative;
  border-radius: 10px;
}

.actives_pro .lis .proimgs image {
  width: 100%;
}

.actives_pro .lis .procenter {
  /* display: -webkit-box; *//* -webkit-line-clamp: 2; *//* -webkit-box-orient: vertical; */
  overflow: hidden;
  line-height: 36rpx;
  /*height: 32px;*/
  color: #000;
  font-size: 14rpx;
  text-align: justify;
  height: 72rpx;
  margin-bottom: 11.25rpx;
}

.actives_pro .lis .proprize {
  width: 95%;
  margin-left: 2.5%;
  overflow: hidden;
  /* height: 2.5rem; */
}

.actives_pro .lis .proprize .ppa {
  /* display: block; */
  font-family: arial;
  font-size: 14px;
  color: #f50;
}

.actives_pro .lis .proprize .ppa text {
  color: #ff4e42;
}

.actives_pro .lis .proprize .moredo {
  width: 60%;
  float: right;
  margin-right: 22.5rpx;
  position: relative;
}

.actives_pro .lis .proprize .moredo .clicle {
  font-size: 22.5rpx;
  line-height: 11.25rpx;
}

.actives_pro .lis .procenter navigator {
  font-size: 27.75rpx;
  /*font-weight:600;*/
}

.actives_pro .lis .proprize .moredo .howdo {
  width: 90%;
  height: 0.6rem;
  border-radius: 0.3rem;
  background-color: #878787;
  display: none;
  position: absolute;
  left: 0;
}

.actives_pro .lis .proprize .moredo .howdo image {
  width: 0.4rem;
  margin-top: 0.15rem;
}

.int_foot{
width:100%;
overflow:hidden;
height:115rpx;
background-color: white;
border-top: 1px solid #e1e1e1;
padding-top: 20rpx;
padding-bottom: 30rpx;
}

.int_foot .foot_one{
width: 25%;
float: left;
height: 100%;
text-align: center;
}

.int_foot .foot_one image{
width:100rpx;
height: 100rpx;
}

.int_foot .foot_one navigator{
display: block;
width: 100%;
height: 100%;
}

.int_foot .foot_one navigator view{
width: 100%;
height: 84rpx;
overflow: hidden;
margin-bottom: 15rpx;
}

.int_foot .foot_one navigator view image{
width: 80rpx;
height: auto;
margin-top: 5rpx;
}

.int_foot .foot_one navigator view text{
line-height: 30rpx;
}

.one_gg navigator{
  width: 100%;
}

.two_gg navigator{
  width: 50%;
  float: left;
}
.three_gg navigator{
  width: 33.33%;
  float: left;
}
.four_gg navigator{
  width: 25%;
  float: left;
}
.five_gg navigator{
  width: 20%;
  float: left;
}
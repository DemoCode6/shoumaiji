	margin-top: 55rpx;
	color: #585858;
	font-size: 30rpx;
}

.spec_num_w .spec_num_w1 {
	padding-left: 25rpx;
}

/* 加减主容器 */

.stepper {
	width: 184rpx;
	height: 65rpx;
	/*给主容器设一个边框*/
	border: 2rpx solid #ccc;
	border-radius: 6rpx;
	display: inline-block;
	float: right;
  margin-right: 20rpx;
}

/*加号和减号*/

.stepper text {
	width: 60rpx;
	line-height: 65rpx;
	text-align: center;
	float: left;
}

/*数值*/

.stepper input {
	width: 60rpx;
	height: 67rpx;
	float: left;
	margin: 0 auto;
	text-align: center;
	font-size: 32rpx;
	/*给中间的input设置左右边框即可*/
	border-left: 2rpx solid #ccc;
	border-right: 2rpx solid #ccc;
	padding-top: 5rpx;
	box-sizing: border-box;
}

/*普通样式*/

.stepper .normal {
	color: black;
}

/*禁用样式*/

.stepper .disabled {
	color: #ccc;
}

/* 确定 */
.spec_submit {
	width: 100%;
	background: #fd5b59;
	color: #fff;
	height: 100rpx;
	line-height: 100rpx;
	font-size: 32rpx;
	text-align: center;
	position: absolute;
	bottom: 0;
	left: 0;
}

/* 购物车_从底部弹出选择框 */
.element-wrapper {
	display: flex;
	position: fixed;
	left: 0;
	top: 0;
	height: 100%;
	width: 100%;
	overflow: hidden;
	z-index: 2000;
}

.background {
	background-color: rgba(0, 0, 0, 0);
	width: 100vw;
	height: 100vh;
	position: absolute;
	top: 0;
}

.bg {
	background-color: rgba(0, 0, 0, 0.4);
	width: 100vw;
	height: 100vh;
	position: absolute;
	top: 0;
	bottom: 110rpx;
	transition-property: background-color;
	transition-timing-function: ease;
	transition-duration: 1s;
	transition-delay: 0;
	z-index: 1999;
}
.element {
	position: fixed;
	width: 100%;
	height: 758rpx;
	bottom: 0;
	background-color: rgba(255, 255, 255, 1);
}



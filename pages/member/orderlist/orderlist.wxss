/* pages/carlist/carlist.wxss */
.heads{
	width: 100%;
	height: 37.5rpx;
	padding: 15rpx 0;
	background-color: #fff;
}

.heads navigator{
	width: 16%;
	float: left;
	height: 37.5rpx;
	border-right: 1px solid #ff4e42;
	text-align: center;
	line-height: 37.5rpx;
  font-size: 26.25rpx;
}

.heads navigator:last-child{
	border: none;
}

.heads navigator.ffa text{
	border-bottom: 1px solid #ff4e42;
	padding-bottom: 11.25rpx;
}

.heads view{
	width: 16%;
	float: left;
	height: 37.5rpx;
	border-right: 1px solid #ff4e42;
	text-align: center;
	line-height: 37.5rpx;
  font-size: 26.25rpx;
}

.heads view:last-child{
	border: none;
}

.heads view.ffa text{
	border-bottom: 1px solid #ff4e42;
	padding-bottom: 11.25rpx;
}

.orders{
	width: 95%;
	overflow: hidden;
	margin: 11.25rpx auto;
}

.orders .aa{
	width: 98%;
	overflow: hidden;
	border: 1px solid #E5E5E5;
	border-radius: 5px;
	padding: 22.5rpx 0;
	margin: 11.25rpx auto;
	background-color: white;
}

.orders .aa .wlstate{
	width:95%;
	margin-left:2.5%;
  font-size: 26.25rpx;
}

.orders .aa .single{
	width:100%;
	overflow:hidden;
	margin-top:11.25rpx;
}

.orders .aa .omg{
	width: 112.5rpx;
	height: 112.5rpx;
	border-radius: 5px;
	overflow: hidden;
	float: left;
	margin: 13.5rpx 13.5rpx;
	border: 1px solid #E5E5E5;
}

.orders .aa .omg image{
	width: 100%;
}

.orders .aa .inf{
	width: 75%;
	float: right;
	overflow: hidden;
}

.orders .aa .inf .nns{
	font-size: 26.25rpx;
	display: -webkit-box;
	-webkit-line-clamp: 2;
	-webkit-box-orient: vertical;
	overflow: hidden;
	text-overflow: ellipsis;
	margin-top:11.25rpx;
}

.orders .aa .inf .prize,.orders .aa .how{
	text-align: right;
	margin-top: 15rpx;
}

.orders .aa .inf .prize .sl{
	color: #c3c3c3;
	margin-right: 112.5rpx;
  font-size: 26.25rpx;
}

.orders .aa .inf .prize .hh{
	color: #ff5044;
	margin-right: 37.5rpx;
  font-size: 26.25rpx;
}

.orders .aa  .how text,.orders .aa  .how navigator{
	display: inline-block;
	height: 52.5rpx;
	padding: 0 11.25rpx;
	border-radius: 56.25rpx;
	line-height: 52.5rpx;
	font-size: 22.5rpx;
	width: 112.5rpx;
	text-align: center;
}



.orders .aa .how text.del{
	border: 1px solid #b7b7b7;
	color: #b7b7b7;
	margin-top: 11.25rpx;
	margin-right: 11.25rpx;
}

.orders .aa .how text.pays,.orders .aa .how navigator{
	border: 1px solid #ff837b;
	color: #ff837b;
	margin-right: 11.25rpx;
	margin-top: 11.25rpx;
  background-color:white;
  /* float: right; */
}



.orders .aa .how text.pays a{
	color: #ff837b;
}

.orders .aa .how form{
display: inline-block;
height: 52rpx;
padding: 0 11.25rpx;
width: 112.5rpx;
text-align: center;
/* color: #b7b7b7; */
margin-top: 11.25rpx;
margin-right: 11.25rpx;
position: relative;
background-color: white;
border: none;
}

.orders .aa .how form button{
width: 100%;
height: 100%;
color: #ff837b;
outline: none;
background-color: white;
margin: 0;
padding: 0;
line-height: 50rpx;
font-size: 22.5rpx;
position: absolute;
left: 0;
bottom: -18rpx;
border: 1px solid #ff837b;
border-radius: 56.25rpx;
box-sizing:initial;
}
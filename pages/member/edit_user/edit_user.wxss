.changg{
	width: 95%;
	border: 1px solid #f5f5f5;
	height: 75rpx;
	border-radius: 5px;
	margin: 36.5rpx 2.5%;
	background-color: white;
}

.changg input{
	width: 90%;
	float: left;
	text-indent: 0.5em;
	height: 98%;
}

.changg image{
	float: right;
	margin-top: 22.5rpx;
	margin-right: 22.5rpx;
  width: 30rpx;
}

.bnts{
	width: 95%;
	height:60rpx;
	border-radius: 30rpx;
	background-color: #ff5637;
	margin-left: 2.5%;
	overflow: hidden;
	margin-top: 33.75rpx;
	text-align: center;
	line-height: 60rpx;
}

.bnts a{
	display: block;
	width: 100%;
	height: 100%;
	background-color:#ff5637;
	color: white;
	border: none;outline: none;
	font-size: 1rem;
}

.bnts button{
	width: 100%;
	height: 100%;
	background-color:#ff5637;
	color: white;
	border: none;outline: none;
  font-size: 26.25rpx;
}
.pros{
	width: 95%;
  margin-left: 2.5%;
  overflow: hidden;
  margin-top:11.25rpx;
}

.pros .pros_lis{
	width: 100%;
	overflow: hidden;
	margin-bottom: 11.25rpx;
	padding-top: 11.25rpx;
	padding-bottom: 11.25rpx;
	border-bottom: 1px solid #f5f5f5;
	background-color:white;
}

.pros .pros_lis>navigator{
	display: block;
	width: 90%;
	overflow: hidden;
	float: left;
}

.pros .pros_lis .left{
	width: 20%;
	float: left;
	position: relative;
	overflow: hidden;
	height: 112.5rpx;
	margin-left: 11.25rpx;
}

.pros .pros_lis .left image{
	width: 100%;
}

.pros .pros_lis .right {
    width: 76%;
    float: right;
}

.pros .pros_lis .right .bt {
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    overflow: hidden;
    text-overflow: ellipsis;
    font-size: 26.25rpx;
}

.pros .pros_lis .right .name {
    font-size: 26.25rpx;
    color: #838383;
    margin-top: 0.15rem;
}

.pros .pros_lis .right .name span{
	font-size: 24rpx;
	color: #838383;
}

.pros .pros_lis .nun{
	width: 10%;
	float: left;
}

.pros .pros_lis .nun image{
	width: 52.5rpx;
	float: right;
	margin-right:11.25rpx;
	margin-top: 75rpx;
}

.pros .pros_lis .right .prices {
    color: #ff4e42;
    font-size: 30rpx;
    height: 45rpx;
}


.pros .pros_lis .right .zan{
	width: 95%;
	overflow: hidden;
	padding: 11.25rpx 0;
	margin-left: 2.5%;
}

.pros .pros_lis .right .zan span{
	color:#b7b7b7;
}

.pros .pros_lis .right .zan .icona{
	width:48.75rpx;
	float: left;
	/* text-align: right; */
}

.pros .pros_lis .right .zan .icona image{
	width: 37.5rpx;
}

.pros .pros_lis .right .zan .times{
	float: right;
}

.pros .pros_lis .right .pla{
	width: 100%;
	overflow: hidden;
	margin-top: 9.75rpx;
}

.pros .pros_lis .right .pla .op{
	width: 100%;
	float: left;
	overflow: hidden;
	line-height: 75rpx;
	margin-left: 7.5rpx;
	margin-bottom: 11.25rpx;
}

.pros .pros_lis .right .pla .op .peopa{
	width: 75rpx;
	height:75rpx;
	border-radius: 50%;
	position: relative;
	text-align: center;
	float: left;
	margin-right: 11.25rpx;
}

.pros .pros_lis .right .pla .op .peopa .vip{
	position: absolute;
	width: 0.8rem;
	top: 0;
	right: -0.3rem;
	height: auto !important;
}

.pros .pros_lis .right .pla .op .peopa img{
	width: 100%;
	height: 100%;
	border-radius: 50%;
}

.pros .pros_lis .right .pla .opa{
	width: 95%;
	/* float: right; */
	display: -webkit-box;
	-webkit-line-clamp: 2;
	-webkit-box-orient: vertical;
	overflow: hidden;
	text-overflow: ellipsis;
	/* margin-top: 0.5rem; */
	margin-left: 2.5%;
}

.pros .pros_lis .right .pla .opa view{
	color:#666;
}
